// Find the sum of the even numbered values that do not exceed four million.

function fibonacci(n) {
  var a = 0, b = 1, f = 1;
  for(var i = 2; i <= n; i++) {
    f = a + b;
    a = b;
    b = f;
  }
  return summedValueOfArray;
}
