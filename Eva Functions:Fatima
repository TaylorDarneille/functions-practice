// Write a function calculateCube that takes a single number and prints the volume of a cube made from that number.
 function calculateCube(number) {
    return number ** 3;
}
console.log(calculateCube(5));

// Write a function isAVowel that takes a character (i.e. a string of length 1) and returns true if it is a vowel, false otherwise. The vowel could be upper or lower case.
function isAVowel(character) {
  const vowels = ["a", "e", "i", "o", "u"];
  if (vowels.includes(character.toLowerCase())) {
    return true;
  } else {
    return false;
  }

}
console.log(isAVowel("a"));

// Write a function getTwoLengths that accepts two parameters (strings). The function should return an array of numbers where each number is the length of the corresponding string.
function getTwoLengths(par1, par2) {
  return [par1.length, par2.length];
  
}
console.log(getTwoLengths("Hank", "Hippopopalous"));

// Write a function getMultipleLengths that accepts a single parameter as an argument: an array of strings. The function should return an array of numbers where each number is the length of the corresponding string.
function getMultipleLengths(strArray) {
  const lengthsArray = [];
  for (let i = 0; i < strArray.length; i++) {
    lengthsArray.push(strArray[i].length);
  } 
  return lengthsArray;
}
console.log(getMultipleLengths(["hello", "what", "is", "up", "dude"]));


